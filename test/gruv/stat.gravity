#include 'src/optional/assertion/assertion.gravity'
#unittest {};

test('Returned stat instance contains file size', func () {
    Gruv.mkdir('/tmp/aTest', func() {
        Gruv.stat('/tmp/aTest', func(err, result) {
            Assert.equals(4096, result['size']);
        });
    });

    Gruv.startLoop();
});

func main() {}
